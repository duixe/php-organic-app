    "jquery": "^3.4.1",
    "laravel-mix": "^5.0.0",
    "popper.js": "^1.16.0",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.23.1",
    "sass-loader": "^8.0.0",
    "slick-carousel": "^1.8.1",
